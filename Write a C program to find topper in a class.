#include <stdio.h>

int main() {
    int n, i, marks[50], max;

    printf("Enter number of students: ");
    scanf("%d", &n);

    for (i = 0; i < n; i++)
        scanf("%d", &marks[i]);

    max = marks[0];
    for (i = 1; i < n; i++)
        if (marks[i] > max)
            max = marks[i];

    printf("Topper marks = %d", max);
    return 0;
}
