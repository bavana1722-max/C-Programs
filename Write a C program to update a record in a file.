#include <stdio.h>
#include <string.h>

int main()
{
    FILE *fp, *temp;
    char oldName[20], newName[20], fileName[20];

    fp = fopen("students.txt", "r");
    temp = fopen("temp.txt", "w");

    scanf("%s %s", oldName, newName);

    while (fscanf(fp, "%s", fileName) != EOF)
    {
        if (strcmp(fileName, oldName) == 0)
            fprintf(temp, "%s\n", newName);
        else
            fprintf(temp, "%s\n", fileName);
    }

    fclose(fp);
    fclose(temp);

    remove("students.txt");
    rename("temp.txt", "students.txt");

    printf("Record Updated");
    return 0;
}
