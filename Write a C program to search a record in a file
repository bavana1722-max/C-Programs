#include <stdio.h>
#include <string.h>

int main()
{
    FILE *fp;
    char name[20], fileName[20];
    int found = 0;

    fp = fopen("students.txt", "r");
    scanf("%s", name);

    while (fscanf(fp, "%s", fileName) != EOF)
    {
        if (strcmp(name, fileName) == 0)
        {
            found = 1;
            break;
        }
    }
    fclose(fp);

    if (found)
        printf("Record Found");
    else
        printf("Record Not Found");

    return 0;
}
